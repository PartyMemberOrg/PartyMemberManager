@model IEnumerable<PartyMemberManager.Dal.Entities.Nation>

        @{
        ViewData["Title"] = "Index";
        }
        
        <h1>列表</h1>
        
        <p>
            <a asp-action="Create">+添加</a>
        </p>
        <table class="table">
            <thead>
                <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Code)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Name)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.CreateTime)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.OperatorId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Ordinal)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.IsDeleted)
                            </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model) {
                <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Code)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.CreateTime)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.OperatorId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Ordinal)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.IsDeleted)
                            </td>
                        <td>
                            <a asp-action="Edit" asp-route-id="@item.Id">修改</a> |
                            <a asp-action="Details" asp-route-id="@item.Id">查看</a> |
                            <a asp-action="Delete" asp-route-id="@item.Id">删除</a>
                        </td>
                </tr>
                }
            </tbody>
        </table>
        @Html.Pager(Model as PagedDataViewModel)


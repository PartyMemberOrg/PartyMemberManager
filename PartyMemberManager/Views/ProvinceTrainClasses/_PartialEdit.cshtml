@model PartyMemberManager.Dal.Entities.ProvinceTrainClass
    <form asp-action="Create">
        <input asp-for="Id" type="hidden" />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <div class="row">
                <div class="@LabelDivDefaultClass">
                    <label asp-for="Year" asp-required="true" class="control-label"></label>
                </div>
                <div class="@InputDivDefaultClass">
                    <input asp-for="Year" class="form-control"  readonly  value="@Model.Year"/>
                    <span asp-validation-for="Year" class="text-danger validate-message-offset @ValidateSpanDefaultClass"></span>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="@LabelDivDefaultClass">
                    <label asp-for="Name" asp-required="true" class="control-label"></label>
                </div>
                <div class="@InputDivDefaultClass">
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger validate-message-offset @ValidateSpanDefaultClass"></span>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="@LabelDivDefaultClass">
                    <label asp-for="Enabled" asp-required="false" class="control-label"></label>
                </div>
                <div class="form-check @InputDivDefaultClass" style="padding-left: 0px;">
                    <div class="layui-form">
                        <div class="layui-form-item" style="margin-bottom:0px;margin-top:-4px">
                            <input type="checkbox" name="Enabled" lay-skin="primary" value="true" title="" @(Model.Enabled ? "checked" : "")>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>